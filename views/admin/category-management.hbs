
                <!-- End of Topbar --> <!-- Begin Page Content -->
          <div class="container-fluid">
            <!-- Page Heading -->
            <h1 class="h3 mb-2 text-gray-800">Tables</h1>
            <p class="mb-4">
              DataTables is a third party plugin that is used to generate the
              demo table below. For more information about DataTables, please
              visit the
              <a target="_blank" href="https://datatables.net"
                >official DataTables documentation</a
              >.
            </p>

            <!-- DataTales Example -->
        
            <div class="card shadow mb-4">
              <div class="card-header py-3">
                <h6 class="m-0 font-weight-bold text-primary">
                  DataTables Example
                </h6>
              </div>
              <div class="card-body">
                <div class="table-responsive">
                   <div class="row">
                     <a class="btn btn-primary ml-3 mb-2" href="/admin/addcategoryFrm">Add Category</a></div>
                  <table
                    class="table table-bordered"
                    id="dataTable"
                    width="100%"
                    cellspacing="0"
                  >
                    <thead>
                      <tr>
                        <th>Category</th>
                        <th>Brand</th>
                        <th>Add Brands</th>
                        <th>Remove Brands</th>
                        <th>Delete category</th>
                        {{!-- <th>Salary</th>  --}}
                      </tr>
                    </thead>
                    <tfoot>
                      <tr>
                       <th>Category</th>
                        <th>Brand</th>
                        <th>Add Brands</th>
                        <th>Remove Brands</th>
                        <th>Delete category</th>
                        {{!-- <th>Salary</th> --}}
                      </tr>
                    </tfoot>
                    <tbody>
                      {{#each category}}
                      <tr>
                        
                        <td>{{this.category}}</td>
                        <td><select class="form-control required form-control-sm" name="RAM"> 
                                         <option>select brands</option>
                                         {{#each this.brand}}
                                        <option>{{this}}</option>
                                       {{/each}}
                                    </select></td>        


                   
                      <td><button class="btn btn-primary"  onclick= "window.location.href='/admin/addBrandFrm/{{this._id}}'">Add Brands</button></td>
                      <td><button class="btn btn-info "   onclick= "window.location.href='/admin/removeBrandFrm/{{this._id}}'">Remove Brands</button></td>
                        <td><button class="btn btn-danger" onclick= "window.location.href='/admin/deleteCategory/{{this._id}}'">Delete Category</button></td>
                     
                      </tr>
                         {{/each}}
                     
                    </tbody>
                  </table>
                </div>
              </div>
            </div>
          </div>
          <!-- /.container-fluid -->
        </div>
        <!-- End of Main Content -->

        